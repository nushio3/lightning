	keywords = {dust, extinction, planets and satellites: formation, protoplanetary disks},
	pages = {53},
	file = {A Fast and Accurate Calculation Scheme for Ionization Degrees in Protoplanetary:/home/nushio/.mozilla/firefox/nt955shw.default/zotero/storage/GMXFHB5Z/2011ApJ...743...html:text/html}
},

