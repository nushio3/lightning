@article{little_galileo_1999,
	title = {Galileo Images of Lightning on Jupiter},
	volume = {142},
	issn = {0019-1035},
	url = {http://adsabs.harvard.edu/abs/1999Icar..142..306L},
	doi = {10.1006/icar.1999.6195},
	abstract = {In October and November of 1997 the Galileo Solid State Imager ({SSI)} detected lightning from 26 storms on the night side of Jupiter. More than half the surface area of the planet was surveyed. The data include images of lightning against moonlit clouds (illuminated by light from Io) and images of the same storm on the day and night sides. The spatial resolution ranged from 23 to 134 km per pixel, while the storms ranged in size up to {\textasciitilde}1500 km. Most storms were imaged more than once, and they typically exhibit many flashes per minute. The storms occur only in areas of cyclonic shear and near the centers of westward jets. Latitudes near 50 deg in both hemispheres are particularly active, although the northern hemisphere has more lightning overall. The greatest optical energy observed in a single flash was 1.6x1010 J, which is several times larger than terrestrial superbolts. The average optical power per unit area is 3x 10-7 W m-2, which is close to the terrestrial value. The limited color information is consistent with line and continuum emission from atomic hydrogen and helium. The intensity profiles of resolved lightning strikes are bell-shaped, with the half-width at half-maximum ranging from {\textasciitilde}45 to 80 km. We used these widths to infer the depth of the strikes, assuming that the appearance of each is the result of light scattering from a point source below the cloudtops. We conclude that lightning must be occurring within or below the jovian water cloud. The occurrence of lightning in regions of cyclonic shear has important implications for the dynamics of Jupiter's atmosphere.},
	urldate = {2013-03-07},
	journal = {Icarus},
	author = {Little, Blane and Anger, Clifford D. and Ingersoll, Andrew P. and Vasavada, Ashwin R. and Senske, David A. and Breneman, H. Herbert and Borucki, William J. and Galileo {SSI} Team},
	month = dec,
	year = {1999},
	pages = {306--323}
},

@article{king_radial_1995,
	title = {Radial models of mantle viscosity: results from a genetic algorithm},
