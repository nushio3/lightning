	keywords = {dust, extinction, planetary systems: formation, planetary systems: protoplanetary disks},
	pages = {1247--1263},
	file = {Numerical Modeling of the Coagulation and Porosity Evolution of Dust Aggregates:/home/nushio/.mozilla/firefox/nt955shw.default/zotero/storage/3QDXHDDZ/2009ApJ...707.html:text/html}
